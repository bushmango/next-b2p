{
  "name": "next-b2p",
  "version": "1.0.0",
  "scripts": {
    "1": "yarn rimraf .next && yarn concurrently \"yarn dev\" \"yarn 2\" \"yarn sidecar\"",
    "2": "yarn css-watch",
    "sidecar": "yarn barrel-rider-sidecar --watch --src components lib common",
    "css-watch": "yarn tsm  \"**/*.module.scss\" --ignore \"**/node_modules\" --watch",
    "css": "yarn tsm  \"**/*.module.scss\" --ignore \"**/node_modules\"",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc"
  },
  "dependencies": {
    "@types/lodash": "4.14.150",
    "@zeit/next-sass": "^1.0.1",
    "barrel-rider-sidecar": "1.0.23",
    "concurrently": "5.2.0",
    "fast-random": "^2.0.4",
    "giphy-api": "^2.0.1",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "next": "9.3.6",
    "node-sass": "4.14.0",
    "ramda": "^0.27.0",
    "ramda-adjunct": "^2.26.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "13.13.4",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.7",
    "husky": "4.2.5",
    "prettier": "2.0.5",
    "pretty-quick": "^2.0.1",
